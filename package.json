{
  "name": "polykey-docs",
  "private": true,
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "lint": "eslint '{src,server}/**/*.{js,ts,jsx,tsx}' './docusaurus.config.ts' './sidebars.ts'",
    "lintfix": "eslint '{src,server}/**/*.{js,ts,jsx,tsx}' './docusaurus.config.ts' './sidebars.ts' --fix",
    "start": "docusaurus start",
    "swizzle": "docusaurus swizzle",
    "build": "docusaurus build --out-dir=./public",
    "deploy": "wrangler publish"
  },
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@cloudflare/workers-types": "^3.14.1",
