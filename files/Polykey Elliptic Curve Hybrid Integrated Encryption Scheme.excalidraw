    {
      "id": "ZsJ1LZPC3sZzQt2Ix5QwJ",
      "type": "text",
      "x": 3911.83519940886,
      "y": 1603.8817830028383,
      "width": 701.5852661132812,
      "height": 260,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
      "fillStyle": "hachure",
      "strokeWidth": 2,
      "strokeStyle": "solid",
      "roughness": 0,
      "opacity": 100,
      "groupIds": [],
